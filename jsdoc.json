{
    "opts": {
        "template": "./jsdocTemplate/tui-jsdoc-template/",
        "encoding": "utf8",
        "destination": "./docs/",
        "tutorials": "./docs/tutorials"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "tags": [],
        "excludeTags": []
    },
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "default": {
            "staticFiles": {
                "include": [
                    "./server/examples",
                    "./jsdocTemplate/images"
                ]
            },
            "layoutFile": ""
        },
        "cleverLinks": false,
        "monospaceLinks": false,
        "logo": {
            "url": "./img/CEEO_Innovations_Logo_White.png",
            "height": "60px",
            "width": "200px"
        },
        "footerText": "SPIKE Web Interface Documentation (2020 September 21)"
    }
}