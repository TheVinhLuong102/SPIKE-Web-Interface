{
    "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "encoding": "utf8",
        "destination": "./server/documentation/"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "tags": [],
        "excludeTags": []
    },
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "default": {
            "staticFiles": {
                "include": [
                    "./server/examples"
                ]
            }
        },
        "cleverLinks": false,
        "monospaceLinks": false,
        "logo": {
            "url": "./img/CEEO_Innovations_Logo_White.png",
            "height": "30px",
            "width": "100px"
        },
        "footerText": "SPIKE Web Interface Documentation (2020 September 21)"
    }
}